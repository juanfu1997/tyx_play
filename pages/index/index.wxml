<import src='../../private/shared/bar/bar.wxml' />
<view class="page {{bg}}">
    <template is="bar" data="{{...bar}}"/>
    <view class="container" style="margin-top: {{bar.titleHeight}}px;">
        <view class="pic-wrapper">
            <image class="pic" src="{{img}}intro.png" mode="widthFix"></image>
            <view class="pic-bg"></view>
        </view>
        <view class="btn-wrapper">
            <image class="btn" bindtap="goList" src="{{img}}btn_list.png" mode="widthFix"></image>
            <view class="btn-bg">
            </view>
        </view>
    </view>
</view>